<h1>Show Projects</h1>
<table>
	<tr>
		<th>Name</th>
		<th>Description</th>
		<th></th>
	</tr>

	<% @projects.each do |project| %>
		<tr>
			<td><%= project.name %></td>
			<td><%= project.description %></td>
			<td><%= link_to 'Show', project_path(project) %></td>
			<td><%= link_to 'Edit', edit_project_path(project) %></td>
			<td><%= link_to 'Delete', project_path(project),
				method: :delete,
				data: {confirm: 'Are you sure?'} %></td>
		</tr>
	<% end %>
</table>